<section>
    <h2 class="center">Miracles</h2>
    <div>
        {{#each miracles as |mir|}}
        <table class="item" data-itemId="{{this.id}}">
            <tr>
                <td class="center"><a class="info-button"><i class="fas fa-info-circle"></i></a></td>
                <td><h3>{{this.name}}</h3></td>
            </tr>
            <tr>
                <th>TN</th>
                <th>Pray</th>
                <th>remove</th>
            </tr>
            <tr>
                <td class="center">{{this.data.data.tn}}</td>
                <td class="center"><a class="cast-miracle"><i class="fas fa-pray" title="Cast {{this.name}}"></i></a></td>
                <td class="center"><a class="item-delete"><i class="fas fa-trash" title="Remove {{this.name}}"></i></a></td>
            </tr>
        </table>
        {{/each}}
    </div>
</section>