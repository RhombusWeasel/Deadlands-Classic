<section>
    <table>
        {{#each list as |item|}}
        <tr class="item" data-id="{{this.id}}">
            <td class="center perc10"><a class="sell-item"><</a></td>
            <td class="center perc60">{{this.name}}</td>
            <td class="right perc10">[{{this.data.data.amount}}]</td>
            <td class="right perc20">{{sum (sum (sum this.data.data.cost '/' this.data.data.box_amount) '*' this.data.data.amount) '*' buy_modifier}}</td>
        </tr>
        {{/each}}
    </table>
</section>